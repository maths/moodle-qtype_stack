    </stackversion>
    <questionvariables>
      <text><![CDATA[n1:rand(3)+2;
  n2:(rand(3)+2)^(n1+1);
  n3:n1+1;
  v:x;
  f:1/(v^(n1/n3));
  J:int(f,v,0,n2);
  correctanswer:if abs(J)>99999 then 99999 else J;
  Jindefinite:int(f,v);
  upperJ:subst(n2,v,Jindefinite);
  lowerJ:subst(0,v,Jindefinite);
  deriv:diff(f,v);
  deriv2:diff(deriv,v);
  derivlimits:int(v,v,0,n2);]]></text>
