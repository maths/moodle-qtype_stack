    <questionvariables>
      <text><![CDATA[a: rand_with_step(-5,5,1);
b: a+rand_with_step(1,5,1);
ddddf: rand_with_step(6,12,1);
err: rand([0.01, 0.001, 0.0001]);

/*[a,b,ddddf,err]: [-1,5,6,0.001];*/

h: ev(h,solve(err=ddddf*(b-a)*h^4/180,h));
ivr: ceiling(abs((b-a)/h));
if oddp(ivr) then iv:ivr+1 else iv:ivr;

