    <questionvariables>
      <text><![CDATA[dt: rand([0.2, 0.25]);


dx: [rand_with_step(3,5,0.2), 
     rand_with_step(4,6,0.2), 
     rand_with_step(5,7,0.2), 
     rand_with_step(6,8,0.2), 
     rand_with_step(7,9,0.2), 
     rand_with_step(8,10,0.2)]*0.5;

t: makelist(i*dt,i,1,length(dx)+1); 
x: makelist(0,i,1,length(dx)+1);
for i:1 thru length(dx) do x[i+1]: x[i] +dx[i];  
/* td: apply(table,append( ["t (\\rm{s})", "x (\\rm{m})"], makelist([t[i],x[i]],i,1,length(x))));*/
td: apply(table, append( [['t, 'x]], makelist([t[i],x[i]],i,1,length(x))));
stack_include("https://raw.githubusercontent.com/mkraska/meclib/main/Maxima/fb_value_EN.mac");

v(i):=(x[i+1]-x[i-1])/(2*dt);
a(i):=(x[i+1]-2*x[i]+x[i-1])/dt^2;
vv: transpose(matrix([v(2), v(4), v(6)]));
av: transpose(matrix([a(2), a(4), a(6)]));

toList(m):= args(transpose(m))[1];]]></text>
