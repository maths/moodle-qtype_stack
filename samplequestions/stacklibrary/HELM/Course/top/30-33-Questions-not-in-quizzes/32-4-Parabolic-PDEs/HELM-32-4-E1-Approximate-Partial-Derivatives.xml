    <questionvariables>
      <text><![CDATA[fn(x,t):=x^3*cos(x*t);
[hx,ht]: rand_selection([0.001,0.01,0.02,0.005],2);
[x,t]: rand_selection([1,2,3],2);
dxf: decimalplaces((fn(x+hx,t)-fn(x,t))/hx,3);
dxxc: decimalplaces((fn(x-hx,t)-2*fn(x,t)+fn(x+hx,t))/hx^2,3);
dtf: decimalplaces((fn(x,t+ht)-fn(x,t))/ht,3);
dtc: decimalplaces((fn(x,t+ht)-fn(x,t-ht))/(2*ht),3);

stack_include("https://raw.githubusercontent.com/mkraska/meclib/main/Maxima/fb_value_EN.mac");]]></text>
