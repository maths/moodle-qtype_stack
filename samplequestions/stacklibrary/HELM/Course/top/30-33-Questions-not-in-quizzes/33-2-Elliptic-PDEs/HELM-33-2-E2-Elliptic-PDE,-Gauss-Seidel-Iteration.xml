    <questionvariables>
      <text><![CDATA[/* Define the grid size */
h : 1/3;

/* Define the boundary values and initial guess */
g: rand([x+y, x-y, y-x, x*y^2,x^2*y^2,(1-x)*y, (1-x)*(1-y)]);
ubd(x,y):= dispdp(ev(g),4); /* this can be used for display only */
ub(x,y):= round(ev(g)*10000)/10000;
u0:rand([0,1/2,1]);
u0d : matrix(
    [ubd(0,1)  , ubd(1/3,1), ubd(2/3,1), ubd(1,1)],
    [ubd(0,2/3),    u12  ,    u22  ,   ubd(1,2/3)],
    [ubd(0,1/3),    u11  ,    u21  ,   ubd(1,1/3)],
    [ubd(0,0)  , ubd(1/3,0), ubd(2/3,0), ubd(1,0)]
);
u : matrix(
    [ub(0,1)  , ub(1/3,1), ub(2/3,1), ub(1,1)],
    [ub(0,2/3),    u0  ,    u0  ,   ub(1,2/3)],
    [ub(0,1/3),    u0  ,    u0  ,   ub(1,1/3)],
    [ub(0,0)  , ub(1/3,0), ub(2/3,0), ub(1,0)]
);


/* First Jacobi iteration */
u[3,2]: 1/4 * (u[4,2] + u[2,2] + u[3,3] + u[3,1]);
u[3,3]: 1/4 * (u[4,3] + u[2,3] + u[3,4] + u[3,2]);
u[2,2]: 1/4 * (u[3,2] + u[1,2] + u[2,3] + u[2,1]);
u[2,3]: 1/4 * (u[3,3] + u[1,3] + u[2,4] + u[2,2]);

U1: float(submatrix(1,4,u,1,4));

/* Second Jacobi iteration */
u[3,2]: 1/4 * (u[4,2] + u[2,2] + u[3,3] + u[3,1]);
u[3,3]: 1/4 * (u[4,3] + u[2,3] + u[3,4] + u[3,2]);
u[2,2]: 1/4 * (u[3,2] + u[1,2] + u[2,3] + u[2,1]);
u[2,3]: 1/4 * (u[3,3] + u[1,3] + u[2,4] + u[2,2]);

U2: float(submatrix(1,4,u,1,4));

stack_include("https://raw.githubusercontent.com/mkraska/meclib/main/Maxima/fb_value_EN.mac");

toList(m):= args(transpose(m))[1];]]></text>
