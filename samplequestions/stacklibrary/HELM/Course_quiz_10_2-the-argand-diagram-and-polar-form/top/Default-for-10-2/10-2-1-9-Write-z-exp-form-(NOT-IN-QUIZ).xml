    <questionvariables>
      <text>r1: rand(10)+2;
r2: rand_with_prohib(3,12,[r1]);
th1: rand([%pi/3, %pi/4, %pi/6]);
th2: rand([ (4*%pi)/3, (7*%pi)/6, (5*%pi)/4]);
z1: r1*exp(%i*th1);
z2: r2*exp(%i*th2);
orderless(%i);
ta1: z1;
ta2: z2;</text>
