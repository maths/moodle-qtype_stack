    <questionvariables>
      <text>a1: rand([2,3,4,5]);
f1(x):=a1*x^2;
a2: expand(f1(x+h)) - f1(x);
a3: simplify(a2/h);
ta1:diff(f1(x),x);

b:rand([2,3,4]);
f2(x):=b*x^3;
b2: expand(f2(x+h)) - f2(x);
b3: simplify(b2/h);
ta2:diff(f2(x),x);

ca:rand([6,7,8]);
cb:rand([3,4,5,6]);
f3(x):=ca*x + cb;
c2: expand(f3(x+h)) - f3(x);
c3: simplify(c2/h);
ta3:diff(f3(x),x);

d1:rand([1]);
f4(x):=d1/x;
d2: expand(f4(x+h)) - f4(x);
d3: simplify(d2/h);
ta4:diff(f4(x),x);

f5(x):=sin(x);
ta5:diff(f5(x),x);</text>
