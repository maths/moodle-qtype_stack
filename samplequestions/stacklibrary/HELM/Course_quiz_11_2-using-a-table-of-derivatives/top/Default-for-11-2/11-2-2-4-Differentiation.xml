    <questionvariables>
      <text>a1:rand([4,5]);
f1: a1*sin(t) + sin(a1*t);
ta1:diff(f1,t);

b1:rand([3,4]);
b2:b1+2;
f2: b1*cos(2*v) - b2*sin(v/2);
ta2:diff(f2,v);

c1:rand([4,6,8]);
c2:c1/2;
f3: c1*e^(2*n) + c2*e^(-2*n) + n^2/2;
ta3:diff( f3,n);

d1:rand([3,5]);
d2:rand([2,4]);
f4: e^(d1*t)/(d1-1) + d2*tan(d2*t)
ta4:diff( f4, t);

e1:rand([2,3,4]);
f5: (r^2 + 1)^2 - e1*e^(-2*r);
ta5:diff(expand(f5),r);</text>
    </questionvariables>
