    <questionvariables>
      <text>f1(x):=x;
g1(x):=tan(x);
df1: diff(f1(x),x);
dg1: diff(g1(x),x);
ta1:diff(f1(x)*g1(x),x);

b1:rand([4,5,6]);
b2:b1-2;
f2(x):=x^b1;
g2(x):=ln(b2*x);
df2: diff(f2(x),x);
dg2: diff(g2(x),x);
ta2:diff(f2(x)*g2(x),x);

f3(x):=sin(x);
g3(x):=sin(x);
df3: diff(f3(x),x);
dg3: diff(g3(x),x);
ta3:diff(f3(x)*g3(x),x);

d1:rand([2,3,4]);
d2:rand([3,4,5]);
f4(x):=e^(d1*x);
g4(x):=cos(d2*x);
df4: diff(f4(x),x);
dg4: diff(g4(x),x);
ta4:diff(f4(x)*g4(x),x);</text>
