    <questionvariables>
      <text>/*Random constants*/
points_x: [-rand(3)+1,rand(3)+1];
y_intercept: rand([-1,1])*(rand(10)+1);

/*Question variables*/
df: 3*x^2+a2*x+a1;
sys: [subst(x=points_x[1],df=0),subst(x=points_x[2],df=0)];
consts: solve(sys,[a1,a2]);
/*[a1,a2]: [rhs(consts[1][1]),rhs(consts[1][2])];*/
cubic: integrate(subst(consts[1],df),x)+y_intercept;
points: makelist([i,subst(x=i,cubic)],i,points_x);

/*Answers*/
ta1: subst(consts[1],df);
ta2: setify(points);</text>
    </questionvariables>
