    <questionvariables>
      <text><![CDATA[/*Randomisation constants*/
const: 3*(rand(4)+1);

/*Question variables*/
f: x^4-1/const*x^3;
df: diff(f,x);
d2f: diff(f,x,2);
points: sort(map(rhs, solve(df,x)));

/*Answers*/
ta1: df;
ta2: points;
ta3: makelist(subst(i,x,d2f),i,ta2);
ta4: [
[[null,false,"(not answered)"],[1,true,"inconclusive"],[2,false,"local minimum"],[3,false,"local maximum"]],
[[null,false,"(not answered)"],[1,false,"inconclusive"],[2,true,"local minimum"],[3,false,"local maximum"]]
];]]></text>
