    <questionvariables>
      <text><![CDATA[/*Randomisation constants*/
const1: rand(3)+2;
const2: rand([3,5]);

/*Auxiliary*/
classname(x) := block([], if(x<0) then return("maximum") else if(x>0) then return("minimum") else if(x=0) then return("point of inflection"));
classification(x) := block([], if(x>0) then return([true,false,false]) else if(x<0) then return([false,true,false]) else if(x=0) then return([false,false,true]));
temp_list(x) := block([x1:x[1],x2:x[2],x3:x[3]], [[1,x1,"minimum"],[2,x2,"maximum"],[3,x3,"point of inflection"]]);
fnull_item: [[null,false,"(not answered)"]];
tnull_item: [[null,true,"(not answered)"]];

/*Function 1*/
f1: x/const1-log(abs(x));
df1: diff(f1,x);
d2f1: diff(f1,x,2);
statp1: solve(df1,x);
class1: makelist(ev(d2f1,i),i,statp1);
classlist1: map(classification,class1);
classlist1: makelist(temp_list(i),i,classlist1);
list1: makelist(append(fnull_item, i),i,classlist1);

ta11: df1;
ta15: length(statp1);
ta12: rhs(statp1[1]);
ta16: ev(f1,statp1[1]);
ta13: class1[1];
ta14: list1[1];
display1: classname(class1[1]);

/*Function 2*/
f2: x^const2;
df2: diff(f2,x);
d2f2: diff(f2,x,2);
statp2: solve(df2,x);
class2: makelist(ev(d2f2,i),i,statp2);
classlist2: map(classification,class2);
classlist2: makelist(temp_list(i),i,classlist2);
list2: makelist(append(fnull_item, i),i,classlist2);

ta21: df2;
ta25: length(statp2);
ta22: rhs(statp2[1]);
ta26: ev(f2,statp2[1]);
ta23: class2[1];
ta24: list2[1];
display2: classname(class2[1]);

/*Function 3*/
f3: (x-1)/((x+1)*(x-2));
df3: diff(f3,x);
d2f3: diff(f3,x,2);
statp3: realroots(df3);
classlist3: temp_list([false,false,false]);
list3: append(tnull_item, classlist3);


ta31: df3;
ta35: length(statp3);
ta32: null;
ta36: null;
ta33: null;
ta34: list3;

/*taa: [[0,false,"When the second derivative doesn't exist"],[1,false,"When the second derivative is zero at the point"],[2,true,"When \\(\\frac{df}{dx}\\) has the same sign around the point"],[3,false,"When \\(\\frac{df}{dx}\\) changes sign at the point"]];*/]]></text>
