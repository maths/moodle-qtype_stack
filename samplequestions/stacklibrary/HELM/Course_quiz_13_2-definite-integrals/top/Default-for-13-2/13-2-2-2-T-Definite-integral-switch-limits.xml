    <questionvariables>
      <text>n: 2 + rand(4);
a: 1 + rand(6);
b1: 0 + rand(2);
b2: b1 + 1 + rand(2);
exp1: x^n + a;

/* Needed for solution */
Fx : integrate(exp1,x);
Fb1: subst(b1, x, Fx);
Fb2: subst(b2, x, Fx);

/* Teacher's answers */
ta1: Fx + c;
ta2: int(exp1, x, b1, b2);
ta3: -ta2;</text>
