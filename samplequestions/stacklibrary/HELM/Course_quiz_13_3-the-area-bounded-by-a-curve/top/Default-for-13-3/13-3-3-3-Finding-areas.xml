    <questionvariables>
      <text>aa: rand([1,1/2, 1/3]);
exp1: aa*x^3;
l1: rand([-1,-2]);
u1: -l1;
ta1:  2*int(exp1, x, 0, l1); 

exp2: cos(2*t);
l2: 0;
u2: 0.5;
ta2: significantfigures(float(int(exp2, t, l2, u2)),4); 

bb: rand([4,9,16]);
exp3: bb-x^2;
l3: rand(2);
u3: abs(sqrt(bb));
ta3: int(exp3, x, l3, u3); 

/*Maxima Plots: don't used here*/
plot1: plot(exp1, [x, l1-1,u1+1], [y, aa*(l1)^(3)-1, aa*(u1)^(3)+1], [box, false], [axes,solid]);
plot2: plot(exp2, [t, l2-0.5,u2+0.5], [y, 0, 1], [box, false], [axes,solid]);
plot3: plot(exp3, [x, l3-1,u3+1], [y, bb-(u3)^(2)-1, bb-(l3)^(2)+1], [box, false], [axes,solid]);</text>
    </questionvariables>
