a3: 3 + rand(4);

/*Question Variables*/
exp1: 1/(x+a1);
exp2: 2*x/(x^2+a2);
exp3: 1/(x-a3);

/*Answers*/
ta1: int(exp1,x)+c;
ta2: int(exp2,x)+c;
ta3: int(exp3,x)+c;</text>
