    <questionvariables>
      <text>exp1: (sin(t))^2;
lb: -%pi * rand(3);
ub: %pi * (1 + rand(2)); 
d: 1 / (ub - lb);
ta1: d * int(exp1, t, lb, ub);

/* Needed for solution */
intf: ratsimp(int((1 - cos(2*t)) / 2, t));
u1: subst(lb, t, intf);
u2: subst(ub, t, intf);</text>
