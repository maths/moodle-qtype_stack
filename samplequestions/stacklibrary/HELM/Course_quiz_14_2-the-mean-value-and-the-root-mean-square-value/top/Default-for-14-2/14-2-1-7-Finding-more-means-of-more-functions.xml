    <questionvariables>
      <text>const1: rand([1,2,3]);
const2: rand([1,2,3]);

f1: sqrt(t+const1);
a1: 0;
b1: 3;
ta1: 1/(b1-a1)*integrate(f1,t,a1,b1);
ta1: decimalplaces(float(ta1),4);

f2: e^(const2*t);
a2: -1;
b2: 1;
ta2: 1/(b2-a2)*integrate(f2,t,a2,b2);
ta2: decimalplaces(float(ta2),4);

f3: const1+e^(const2*t);
a3: -1;
b3: 1;
ta3: 1/(b3-a3)*integrate(f3,t,a3,b3);
ta3: decimalplaces(float(ta3),4);</text>
    </questionvariables>
