    <questionvariables>
      <text>ordergreat(x, y);
a: rand([2, 4, 6, 8]); 
b: 1 + rand(9);
exp: x + a * y + b;

f_x: diff(exp, x);
f_y: diff(exp, y);

f_xx: diff(f_x, x);
f_yy: diff(f_y, y);
f_xy: diff(f_x, y);
f_yx: diff(f_y, x);

x1: rand_with_prohib(-3, 3, []);
x2: rand_with_prohib(-3, 3, [x1]);
x3: rand_with_prohib(-3, 3, [x1, x2]);
y1: rand_with_prohib(-3, 3, []);
y2: rand_with_prohib(-3, 3, [y1]);
y3: rand_with_prohib(-3, 3, [y1, y2]);

ta1: f_xx;
ta2: f_yy;
ta3: f_xy;
ta4: f_yx;
val1: ev(f_xx, x=x1, y=y1);
val2: ev(f_yy, x=x2, y=y2);
val3: ev(f_xy, x=x3, y=y3);</text>
    </questionvariables>
