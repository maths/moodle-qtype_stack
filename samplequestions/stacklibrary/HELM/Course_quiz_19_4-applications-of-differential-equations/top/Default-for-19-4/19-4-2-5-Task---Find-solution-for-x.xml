    <questionvariables>
      <text>kk: 1;
mm: 1;
nn: 1;
x_0: rand(7)+2;

ta1: (-kk+%i*sqrt(4*mm*nn-kk^2))/(2*mm);
ta2: (-kk-%i*sqrt(4*mm*nn-kk^2))/(2*mm);

pp: sqrt(4*mm*nn-kk^2)/(2*mm);
ta3: e^(-kk*t/(2*mm));
ta4: cos(pp*t);
ta5: sin(pp*t);

x_arb: e^(-kk*t/(2*mm))* ( A*cos(pp*t) + B* sin(pp*t));
d_x: diff(x_arb, t);
d_x_in: subst(0, t, d_x);
sol: solve(d_x_in, B );
BB_val: map(rhs, sol)[1];
B_val: subst(x_0, A, BB_val);
A_val: x_0;

x_sp: subst(A_val, A, x_arb);
x_sp1: subst(B_val, B, x_sp);</text>
    </questionvariables>
