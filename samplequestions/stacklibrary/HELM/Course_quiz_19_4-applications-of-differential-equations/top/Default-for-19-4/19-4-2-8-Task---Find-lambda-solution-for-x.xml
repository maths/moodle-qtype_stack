    <questionvariables>
      <text>kk: 2.5;
mm: 1;
nn: 1;
x_0: rand(7)+2;

ta1: (-kk+sqrt(kk^2-4*mm*nn))/(2*mm);
ta2: (-kk-sqrt(kk^2-4*mm*nn))/(2*mm);

ta3: %e^(ta1*t);
ta4: %e^(ta2*t);

x_arb:  A* %e^(ta1*t) + B* %e^(ta2*t);
d_x: diff(x_arb, t);
x_arb_in: subst(0,t, x_arb);     /* apply initial condition t=0 in the arbitrary solution*/
d_x_in: subst(0, t, d_x);      /* apply initial condition t=0 in the derivative of arbitrary solution*/

eq1: x_arb_in=x_0;  /* create the system of equations and solve it*/
eq2: d_x_in = 0;
eqns: [eq1,eq2];
solns: solve(eqns, [A, B] );
soln : solns[1];
sol:map(rhs, soln);

A_val: sol[1];
B_val: sol[2];

x_sp: subst(A_val, A, x_arb);
x_sp1: subst(B_val, B, x_sp);</text>
    </questionvariables>
