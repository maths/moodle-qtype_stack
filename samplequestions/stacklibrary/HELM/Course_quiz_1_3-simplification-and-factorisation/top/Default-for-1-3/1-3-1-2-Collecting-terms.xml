    <questionvariables>
      <text>simp:false;

a1: ev(rand(6)+4, simp);
a2: ev(rand(4)+3, simp);
a3: ev(a1-2, simp);
a4: ev(rand(2)+2, simp);
exp1: a1*w + a2*r - a3*w + a4*r;
ta1: ev(exp1,simp);

b1: ev(rand(1)+2, simp);
b2: ev(rand(5)+4, simp);
b3: ev(b2-1, simp);
exp2: b1*w^2 + b2*w + b3
