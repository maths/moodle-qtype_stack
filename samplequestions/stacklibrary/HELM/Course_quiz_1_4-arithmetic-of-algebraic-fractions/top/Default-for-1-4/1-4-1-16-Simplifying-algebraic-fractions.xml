    <questionvariables>
      <text>a1: rand([3,5,7]);
a2: rand([3,4,5]);
a3: rand([1,2])
a4: rand([3,4])

numerator1: expand((x-4)*(x-a1));
denominator1: expand((x-4)*(x-(a1+2)));
numerator2: expand((x-a2)*(x+a2));
denominator2: expand((x-a2)*(x-8));
numerator3: expand((2*x-3)*(x-a3));
denominator3: expand((2*x-3)*(x-(a3+2)));
numerator4: 5*z-5*a4;
denominator4: expand((z-6)*(z-a4));

ta1: (x-a1)/(x-(a1+2));
ta2: (x+a2)/(x-8);
ta3: (x-a3)/(x-(a3+2));
ta4: 5/(z-6);</text>
    </questionvariables>
