    <questionvariables>
      <text>simp:false;
v: rand([x,z,s,t]);

a1: rand([2,4,6]);
a2: rand([5,7]);
a3: ev(rand(4)+3,simp);
exp1: a1/(v+a3)+a2/(v+1);
num1: ev(expand(a1*(v+1))+expand(a2*(v+a3)),simp);
ta1: num1/((v+a3)*(v+1));
ta1: ev(ta1,simp);

b1: ev(rand(4)+2,simp);
b2: ev(b1+1,simp);
exp2: b1/(b1*v+1)-b2/(b2*v+b1);
num2: ev(expand(b1*(b2*v+b1))-expand(b2*(b1*v+1)),simp);
ta2: num2/((b1*v+1)*(b2*v+b1));
ta2: ev(ta2,simp);

c1: ev(rand(5)+3,simp);
exp3: (v-1)/(v-c1)+(v-1)/(v-c1)^2;
num3: ev(expand((v-1)*(v-c1))+v-1,simp);
ta3: num3/(v-c1)^2;</text>
