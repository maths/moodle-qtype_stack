    <questionvariables>
      <text>simp:false;
v: rand([x,z,s,t]);

a1: rand([3,5,7]);
a2: 4;
a3: ev(a2+1, simp);
a4: ev(a2-1, simp);

exp1: a1*v/a2-v/a3+v/a4;
ta1: fullratsimp(exp1)
ta1: ev(ta1,simp);

exp2: a1*v/a2-(v/a3+v/a4);
ta2: fullratsimp(exp2)
ta2: ev(ta2,simp);</text>
