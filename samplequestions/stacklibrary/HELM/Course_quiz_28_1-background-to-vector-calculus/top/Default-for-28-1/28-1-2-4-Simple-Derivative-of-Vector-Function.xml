    <questionvariables>
      <text><![CDATA[stack_reset_vars(true);
stackvectortex(ex):= block(sconcat("{\\underline{", tex1(first(args(ex))), "}}"));

ordergreat(i,j);
ijk_vec: matrix([i],[j]);

aa:rand(20)+3;
bb:rand(10)+13;
cc:rand(10)+2;

matr:matrix([aa*t],[(bb*t-cc*t^2)]);
vec_display:texboldatoms(dotproduct(ijk_vec,matr));

unit_perp_i:(bb-cc)/sqrt(aa^2+(bb-cc)^2);
unit_perp_j:-aa/sqrt(aa^2+(bb-cc)^2);
ta1:aa;
ta2:bb-2*cc*t;
ta3:0;
ta4:-2*cc;

ta5:bb/cc;

ta6:unit_perp_i;
ta7:-unit_perp_i;
ta8:unit_perp_j;
ta9:-unit_perp_j;]]></text>
