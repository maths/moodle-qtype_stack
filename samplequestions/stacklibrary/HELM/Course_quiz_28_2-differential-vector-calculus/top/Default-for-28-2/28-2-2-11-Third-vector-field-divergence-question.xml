    <questionvariables>
      <text>ordergreat(x,y,z);
aa:rand(4)+2;
bb:rand(4)+2;
cc:rand(4)+2;
dd:rand(2)+2;
orderless(aa*z*x^(aa-1),2*bb*y^(bb-1)*z^cc,dd*x*y*z^(dd-1))

alpha:rand(3)+2;
beta:rand(3)+2;
gamma:rand(3)+2;
ta1:aa*z*x^(aa-1)-2*bb*y^(bb-1)*z^cc+dd*x*y*z^(dd-1);
simp:false;
interim_sub(x,y,z):=ta1;
interim_expr = interim_sub(alpha,beta,gamma)
ta2:subst(alpha, x, subst(beta, y, subst(gamma, z, ta1)));
simp:true;</text>
    </questionvariables>
