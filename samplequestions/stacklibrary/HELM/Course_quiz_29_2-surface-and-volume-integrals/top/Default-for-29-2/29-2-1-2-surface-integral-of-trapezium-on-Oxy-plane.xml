    <questionvariables>
      <text>ymax:rand(4)+1;
xlowmax:rand(4)+3;
xupmax:rand_with_prohib(2,6,[xlowmax, xlowmax+1, xlowmax+2, xlowmax+3]);
xcoef:rand(4)+3;


ta1i:0;
ta1j:0;
ta1k:1;
ta2inlow:0;
ta2inup:y*(xupmax-xlowmax)/ymax + xlowmax;
ta2outlow:0;
ta2outup:ymax;
ta3i:0;
ta3j:0;
ta3k:integrate(integrate(xcoef*x, x, ta2inup, ta2inlow), y, ta2outup, ta2outlow);

First_int_indef:integrate(xcoef*x, x);
First_int_def:integrate(xcoef*x, x, ta2inlow, ta2inup);
Second_int_indef:integrate(First_int_def, y);
Second_int_def:integrate(First_int_def, y, ta2outlow, ta2outup);</text>
