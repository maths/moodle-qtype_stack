fx:x + a;
gx:b*x + mc;

gfx:b*fx-c;
expgfx: expand(gfx);
ta1: expgfx

fgx: gx + a;
expfgx: expand(fgx);
ta2:expfgx;

wronggfx:ev(-b*(x+a)-c,simp);
wrongfgx:ev((-b*(x)-c)+a,simp);
gxplusfx:exp(fx+gx);
gxmulfx:exp(fx*gx);
 sc:sconcat(c);
 smb:sconcat(mb);
 smc:sconcat(mc);</text>
