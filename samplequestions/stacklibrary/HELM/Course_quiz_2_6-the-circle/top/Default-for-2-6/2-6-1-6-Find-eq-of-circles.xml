    <questionvariables>
      <text>ordergreat(x,y);

/* coordinates of the centers*/
x1: 0;
y1: rand(4)+2;
x2: -y1;
y2: 0;
x3: -(rand(3)+2);
y3: rand_with_prohib(3,6,[y1]);
x4: 1;
y4:1;

/*radious of the cirles*/
r1: rand(3)+2;
r2: rand_with_prohib(3,7,[r1]);
r3: rand_with_prohib(3,8,[r1,r2]);
r4: rand([sqrt(3), sqrt(5), sqrt(7)]);

ta1: (x-x1)^2+(y-y1)^2 = r1^2;
ta2: (x-x2)^2+(y-y2)^2 = r2^2;
ta3: (x-x3)^2+(y-y3)^2 = r3^2;
ta4: (x-x4)^2+(y-y4)^2 = r4^2;</text>
