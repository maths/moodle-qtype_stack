    <questionvariables>
      <text>ordergreat (x,y);
/* Define centres*/
x1: rand(3)+4;
y1: -(x1+1);
x2: 1;
y2: rand([-2,-3,-4]);
x3: rand([2,3,4])
y3: 0;

sipm:false;
/* Completed square*/ 
exp1: (x-x1)^2-x1^2;
exp2: (y-y1)^2-y1^2;
c1: expand(exp1+exp2)=0;
r1: ev(sqrt(x1^2+y1^2),simp);

exp3: (x-x2)^2-x2^2;
exp4: (y-y2)^2-y2^2;
aa: ev(x2+y2+rand(5)+5, simp);
c2: expand(exp3+exp4)=aa;
r2: ev(sqrt(aa+x2^2+y2^2),simp);

exp5: (x-x3)^2-x3^2;
exp6: (y-y3)^2-y3^2;
bb: ev(rand(5)+10, simp);
c3: expand(exp5+exp6)-bb=0;
r3: ev(sqrt(bb+x3^2+y3^2),simp);
simp:true;</text>
    </questionvariables>
