B:subset(BB,evenp);
C:subset(BB,oddp);
AcupB:union(A,B);
AcapB:intersection(A,B);
ta1:intersection(C,union(A,B));
ta2:union(C,intersection(A,B));
ta3:union(A,B,C);</text>
