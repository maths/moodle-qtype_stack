ta6:block(s:0, for i:1 thru l do if (L[i] >= 170 and L[i] < 175) then s:s+1, return(s));
ta7:block(s:0, for i:1 thru l do if (L[i] >= 175 and L[i] < 180) then s:s+1, return(s));
ta8:block(s:0, for i:1 thru l do if (L[i] >= 180 and L[i] < 185) then s:s+1, return(s));
