175.3+rand([0,1,2]), 175.3+rand([0,1,2]), 177.8, 175.2+rand([3,4]), 175.4+rand([3,4]), 175.8+rand([3,4]), 
180.2+rand([0,1,2,3,4]), 
185.1+rand([0,1,2,3]), 185.2+rand([0,1,2,3]), 189.4];
L: random_permutation(Lnew);



l:length(L); 
ta1:block(s:0, for i:1 thru l do if (L[i] >= 145 and L[i] < 150) then s:s+1, return(s));
ta2:block(s:0, for i:1 thru l do if (L[i] >= 150 and L[i] < 155) then s:s+1, return(s));
ta3:block(s:0, for i:1 thru l do if (L[i] >= 155 and L[i] < 160) then s:s+1, return(s));
