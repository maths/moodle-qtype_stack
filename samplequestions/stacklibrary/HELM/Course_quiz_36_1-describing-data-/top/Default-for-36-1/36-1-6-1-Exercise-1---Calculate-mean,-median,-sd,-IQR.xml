L:sort(L);
r1:float((L[4]+L[5])/2);
r3:float((L[12]+L[13])/2);
ta1:decimalplaces(float(mean(L)),3);
ta2:decimalplaces(float(std1(L)),3);
ta3: decimalplaces(float(median(L)),1);
ta4: decimalplaces(r3-r1,1);
ta5:true;
m:lsum(i,i,L)/length(L);
v:float(var1(L)*(length(L)-1));
l4:L[4];l5:L[5];l8:L[8]; l9:L[9];l12:L[12];l13:L[13];</text>
