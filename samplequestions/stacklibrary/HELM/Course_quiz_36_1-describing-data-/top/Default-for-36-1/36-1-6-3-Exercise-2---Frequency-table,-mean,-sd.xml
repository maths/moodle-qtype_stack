57,169,204,145,248+rand([0,100]),38,89,146,157,201,96+rand([0,100]),156];  
l:length(L); 
ta1:block(s:0, for i:1 thru l do if (L[i] >= 0 and L[i] < 100) then s:s+1, return(s));
ta2:block(s:0, for i:1 thru l do if (L[i] >= 100 and L[i] < 200) then s:s+1, return(s));
ta3:block(s:0, for i:1 thru l do if (L[i] >= 200 and L[i] < 300) then s:s+1, return(s));
ta4:block(s:0, for i:1 thru l do if (L[i] >= 300 and L[i] < 400) then s:s+1, return(s));
ta5:block(s:0, for i:1 thru l do if (L[i] >= 400 and L[i] < 500) then s:s+1, return(s));
ta6:block(s:0, for i:1 thru l do if (L[i] >= 500 and L[i] < 600) then s:s+1, return(s));

sum:lsum(i,i,L);
v: var1(L);
sqdev:float(v*(l-1));
ta7:decimalplaces(float(mean(L)),2);
ta8:decimalplaces(float(std1(L)),2);]]></text>
