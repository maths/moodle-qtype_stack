ta1: decimalplaces(((lambda^5)*exp(-lambda))/120, 5);
ta2: decimalplaces(((lambda^9)*exp(-lambda))/362880, 5);
ta3: decimalplaces(exp(-lambda)*(1 + lambda + lambda^(2)/2 + lambda^(3)/6 + lambda^(4)/24), 5);
ta4: decimalplaces(1 - ta3, 5);</text>
