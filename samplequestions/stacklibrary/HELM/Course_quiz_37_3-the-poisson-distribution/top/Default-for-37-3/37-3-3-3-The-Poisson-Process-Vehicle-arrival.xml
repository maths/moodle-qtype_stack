    <questionvariables>
      <text>a1:360*(1+rand(3));
a2:10*(1+rand(3));
lambda:(a1/360)*(a2/10);
Peq5: float(pdf_poisson(5,lambda));
Pleq4: float(cdf_poisson(4,lambda));
ta1: round(100*Peq5);
ta2: round(100*Pleq4);
ta3: round(100*(1-Peq5-Pleq4));

