    <questionvariables>
      <text>a:rand([1,2,3,4,5]);
k:2/(a^2);
f_x(u):=(2/(a^2))*u;
ta2:u^2/(a^2);
ta3: integrate(f_x(u)*u,u,0,a);

s1: integrate(f_x(u)*u^2,u,0,a);
ta4: s1-ta3^2;
ta5: integrate(f_x(u)*%e^u,u,0,a);
ta5:fullratsimp(ta5);
s2: integrate(f_x(u)*%e^(2*u),u,0,a);
ta6: fullratsimp(s2- ta5^2);
ta7: (log(u)^2)/a^2;
ta8: (2*log(u))/(a^2*u);</text>
