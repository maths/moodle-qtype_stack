    <questionvariables>
      <text>numa:rand([1,2,3]);
numb:decimalplaces(rand_with_step(2.48,2.68,0.01),2);
numc:rand([1,2,3]);
numd:decimalplaces(rand_with_step(-1.78,-1.38,0.01),2);
nume:decimalplaces(rand_with_step(-0.98,-0.58,0.01),2);
numd2:-numd
nume2:-nume
enta:decimalplaces(float(cdf_normal(numa,0,1)-0.5),4);
entb:decimalplaces(float(cdf_normal(numb,0,1)-0.5),4);
entc:decimalplaces(float(cdf_normal(numc,0,1)-0.5),4);
entd2:decimalplaces(float(cdf_normal(numd2,0,1)-0.5),4);
ente2:decimalplaces(float(cdf_normal(nume2,0,1)-0.5),4);
ta1:decimalplaces(float(enta+0.5),4)
ta2:decimalplaces(float(0.5-entb),4);
ta3:entc
ta4:decimalplaces(float(entd2-ente2),4);</text>
