    <questionvariables>
      <text>mu:2000
sigma:40
z: decimalplaces(float(rand_with_step(2.47,2.67,0.01)),2);
prob: decimalplaces(float(cdf_normal(z,0,1)-0.5),4);
numa: decimalplaces(100*2*prob,2);

ta1: -z;
ta2: z;
ta3: decimalplaces(float(mu-sigma*z),0);
ta4: decimalplaces(float(mu+sigma*z),0);

checkz1: decimalplaces(float(quantile_normal(0.5 - numa/200,0,1)),2);
checkz2: decimalplaces(float(quantile_normal(0.5 + numa/200,0,1)),2);
checkt1: decimalplaces(float(quantile_normal(0.5 - numa/200,mu,sigma)),0);
checkt2: decimalplaces(float(quantile_normal(0.5 + numa/200,mu,sigma)),0);</text>
    </questionvariables>
