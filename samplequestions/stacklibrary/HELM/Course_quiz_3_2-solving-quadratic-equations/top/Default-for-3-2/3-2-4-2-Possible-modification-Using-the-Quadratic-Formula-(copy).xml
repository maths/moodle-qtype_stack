    <questionvariables>
      <text>aa: rand(5)+2;
r1:1/aa;
r2: rand_with_prohib(0,8,[r1]);
exp:expand(aa*(x-r1)*(x-r2));
ta:solve(exp,x);
ta1:rhs(ta[1]);
ta2:rhs(ta[2]);
bb:-aa*(r1+r2);
cc: aa*r1*r2;</text>
