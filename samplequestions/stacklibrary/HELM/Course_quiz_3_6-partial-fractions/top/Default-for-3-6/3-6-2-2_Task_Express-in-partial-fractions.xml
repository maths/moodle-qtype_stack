expNum1:expand(expA*exp2+expB*exp1);
wholeExp1: expNum1/expDenom1;
ta12_1:var2*exp2+var3*exp1;
ta12_2:var2*exp1+var3*exp2;
ta12_3:var2*(-exp2)+var3*(-exp1);
ta12_4:var2*(-exp1)+var3*(-exp2);

ta13_2:-(rand1*rand2+1)/rand1;
ta13_1:-rand3*rand4/rand2;

ta14_1:expA;
