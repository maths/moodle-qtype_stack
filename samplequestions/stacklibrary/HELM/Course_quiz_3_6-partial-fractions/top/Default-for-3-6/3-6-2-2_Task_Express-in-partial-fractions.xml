ta14_1_1:expB;
ta14_2_1:expA;

ta15_1:expA/exp1+expB/exp2;
ta15_2:(-expA)/(-exp1)-(expB)/(-exp2);


factor1: factor1;
factor2:factor2;
textExp:factor1*factor2;</text>
