rand1:rand_with_step(1,4,1);
rand2:rand_with_step(-3,-1,1);
rand3:rand_with_step(1,3,1);
rand4:rand_with_step(1,5,1);
exp1:rand1*var+(rand1*rand2-1);
exp2:rand3*var+(rand3*rand4-1);
expDenom1:expand(exp1^2);
ta11:exp1^2;
expA:rand_with_prohib(-4,4,[0]);
expB:rand_with_prohib(-4,4,[0]);
expNum1:expand(expA*exp1+expB);
wholeExp1: expNum1/expDenom1;
WholeExpFrac1: expNum1/exp1^2;
ta1:expA/exp1+expB/exp1^2;
factor1:a*var+b;
factor2:(a*var+b)^2;


ta2:var2*exp1+var3;
ta3:expA;
ta4:expB;
ta5:expA/exp1+expB/exp1^2;</text>
    </questionvariables>
