var p = board.create('point', [6.5,4], {name: "A", fixed:true}); 
var q = board.create('point',[6.5,0],{name:"C", fixed:true, label:{autoPosition: true, offset:[10, -10]}});
var r = board.create('point', [0,0], {name: "B", fixed:true, label:{autoPosition: true, offset:[-10, -10]}});
