var angle1 = board.create('angle',["C", "B", "A"], {radius:1.3, name: '&vartheta;', label:{autoPosition: true, offset:[-4, -1]}});
board.create('text', [7, 2, {#AC#}]); 
board.create('text', [3,-0.5, {#BC#}]); 
[[/jsxgraph]]

