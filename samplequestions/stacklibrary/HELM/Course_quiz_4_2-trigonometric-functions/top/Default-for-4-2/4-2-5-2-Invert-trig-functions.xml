    <questionvariables>
      <text>[a1,a2] : rand_selection([-sqrt(3)/2, -sqrt(2)/2, -1/2, 1/2, sqrt(2)/2, sqrt(3)/2],2);
[a3,a4] : rand_selection([-sqrt(3)/2, -sqrt(2)/2, -1/2, 1/2, sqrt(2)/2, sqrt(3)/2],2);
[a5,a6] : rand_selection([-sqrt(3)/3, -1, -sqrt(3), sqrt(3), 1, sqrt(3)/3],2);
ii1 : a1/abs(a1);
ii2 : a2/abs(a2);
ii5 : a5/abs(a5);
ii6 : a6/abs(a6);
ta1 : {asin(a1), ii1*pi - asin(a1)};
ta2 : {asin(a2), ii2*pi - asin(a2)};
ta3 : {acos(a3), -acos(a3)};
ta4 : {acos(a4), -acos(a4)};
ta5 : {atan(a5), atan(a5) - ii5*pi};
ta6 : {atan(a6), atan(a6) - ii6*pi};</text>
    </questionvariables>
