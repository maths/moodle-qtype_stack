rot: rand([1/6, 1/8, 1/9, 1/12]);
angO: 360*rot;

sinB: len1*sin(angO*%pi/180)/len2;
BB: decimalplaces(asin(sinB)*180/%pi,3);
AA: 180-(angO+BB);
OB: decimalplaces(len1*sin(AA*%pi/180)/sin(BB*%pi/180),1);</text>
