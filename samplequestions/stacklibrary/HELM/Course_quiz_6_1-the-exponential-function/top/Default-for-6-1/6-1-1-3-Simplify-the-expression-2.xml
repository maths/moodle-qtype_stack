rr2: rr1^(po1);
ta1: (rr2)*a^((po1*m)+po2);
ta2:  (rr2)*a^((po1*m)+po2-(po3*po4));
simp:false
aa: (rr1*a^m)^(po1);
bb: a^(po2);
cc: (a^(po3))^(po4);</text>
