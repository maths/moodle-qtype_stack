    <questionvariables>
      <text>d1 : 17 + rand(5)
tad : log(d1)

e1: 2+ rand(5)
e2: -3 + rand(3)
tae: log(e1)
exp2 : expand((e^x-e1)*(e^x-e2))
expiny : expand((y-e1)*(y-e2))

f1 : 3 + rand(5)
f12:2*f1
exp3 : e^(2*x)-2*f1*e^(x)+1
tf1 : log(f1+sqrt(f1^2-1))
tf2 : log(f1-sqrt(f1^2-1))</text>
    </questionvariables>
