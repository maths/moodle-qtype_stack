b_mat:matrix([b_x],[b_y])
b_vec: texboldatoms(dotproduct(ijk_vec,b_mat));

 b_x_minus_a_x:b_x-a_x;
 b_y_minus_a_y:b_y-a_y;
AB_mat: matrix([b_x_minus_a_x],[b_y_minus_a_y]);
AB_vec: texboldatoms(dotproduct(ijk_vec, AB_mat));

r:[b_x_minus_a_x,b_y_minus_a_y];
r1:[a_x + b_x,b_y + a_y];
midpoint: [b_x_minus_a_x/2,b_y_minus_a_y/2];
a_ans:[a_x,a_y];
b_ans:[b_x,b_y];

modab: sqrt(b_x_minus_a_x^2 +  b_y_minus_a_y^2);]]></text>
