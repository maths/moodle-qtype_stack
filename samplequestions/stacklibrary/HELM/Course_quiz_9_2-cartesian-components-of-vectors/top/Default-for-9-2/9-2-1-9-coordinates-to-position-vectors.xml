    <questionvariables>
      <text><![CDATA[stack_reset_vars(true);
stackvectortex(ex):= block(sconcat("{\\underline{", tex1(first(args(ex))), "}}"));
ordergreat(i,j);
ijk_vec: matrix([i],[j]);


p_x: 3+rand(10);
q_x: 3+rand(10);

p_y: rand_with_prohib(-10,4,[0,1,-1]); 
q_y:rand_with_prohib(-10,4,[3,4]);

q_mat:matrix([q_x],[q_y]);
q_vec: texboldatoms(dotproduct(ijk_vec,q_mat));

p_mat:matrix([p_x],[p_y]);
p_vec: texboldatoms(dotproduct(ijk_vec,p_mat));

ta1: p_x;
ta2: p_y;
ta3: q_x;
ta4: q_y;]]></text>
    </questionvariables>
