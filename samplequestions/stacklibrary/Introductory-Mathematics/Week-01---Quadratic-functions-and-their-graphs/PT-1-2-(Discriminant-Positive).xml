c:rand_range(-1,1,2)*rand_range(1,8);
expr:(a)*x^2+(b)*x+(c);
disc:b^2-4*a*c;
ta1:disc;
d:signum(disc);
sgnofdisc:["negative","equal to zero","positive"][d+2];
opts:["no real roots","a repeated root","two distinct real roots"];
nums:[0,1,2];
ta2:makelist([nums[i], is(d+2=i),opts[i]],i,1,3);
cor2:[nums[d+2]];]]></text>
