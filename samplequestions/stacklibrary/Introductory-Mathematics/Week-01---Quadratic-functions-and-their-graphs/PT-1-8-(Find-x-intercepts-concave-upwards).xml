    <questionvariables>
      <text><![CDATA[aa: rand(10) + 1;
bb:  rand(10) + 1;

quad: (x+aa)*(x-bb);
qquad: expand(quad);

ta1: solve(quad);
tal2: [[a, true, "concave upwards"], [b, false, "concave downwards"]];
ta2:a;]]></text>
