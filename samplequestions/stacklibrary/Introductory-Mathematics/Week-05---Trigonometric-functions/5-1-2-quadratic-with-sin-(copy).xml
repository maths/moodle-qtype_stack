    </stackversion>
    <questionvariables>
      <text>a: rand([-2,-1,1,2]);
b:rand([-2,-1,1,2]);
c:rand([-1,1]);
d:rand([-1,1]);
fac1:(a*sin(x) +c);
fac2: (b*sin(x) + d);
p:expand(fac1*fac2);
eq: p= 0;
sol1:-c/a;
sol2:-d/b;
xsol1:asin(sol1);
xsol2:asin(sol2);
ta1:{xsol1,xsol2};</text>
