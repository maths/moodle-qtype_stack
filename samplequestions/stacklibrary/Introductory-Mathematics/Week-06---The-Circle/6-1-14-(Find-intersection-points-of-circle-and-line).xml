aa: ev(circ = 0, y=line);
bb:expand(aa);

xx1: solve(bb)[1];
xx2: solve(bb)[2];

yy1: ev(line, x=xx1);
yy2: ev(line, x=xx2);

pp:solve(yy1);
pp2: map(rhs,pp);

p1: (xx1,yy1);
p2: (xx2,yy2);

ta11: sqrt(2);
ta12: 3*sqrt(2);
ta21: -sqrt(2);
 ta22: -3*sqrt(2);</text>
