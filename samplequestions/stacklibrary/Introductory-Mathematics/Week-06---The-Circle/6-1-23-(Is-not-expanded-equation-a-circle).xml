    <questionvariables>
      <text><![CDATA[aa: 1+rand(5);
gg: 1+ rand(5);
ff: 1+rand(5);
cc: 1+rand(30);

circ: aa*x^2 + aa*y^2 +aa*2*gg*x + aa*2*ff*y +aa*cc;
circ1: x^2 + y^2 +2*gg*x + 2*ff*y +cc
ta1: gg^2+ff^2 -cc;

eqn: gg^2+ff^2 -cc;

if (eqn > 0) then ta2: true else ta2: false;]]></text>
