    <questionvariables>
      <text><![CDATA[N:rand([1,2,3,4,5])*1000;
r: rand([3,4,5,6,7]);
m: 1+float(r/100);

an: N*m^n
k:5;
S1:ev(makelist(k,k,1,k),simp);
S2:maplist(lambda([ex],ev(an,n=ex)),S1);
S3:ev(S2,simp);

lim: ev(limit(an,n,inf),simp);

ta1: S3[1]
ta2: decimalplaces(S3[2],2)
ta3: decimalplaces(S3[3],2)
ta4: decimalplaces(S3[4],2)
ta5: decimalplaces(S3[5],2)
ta:[["Convergent",false], ["Divergent",true]];
ta6:"Divergent"]]></text>
