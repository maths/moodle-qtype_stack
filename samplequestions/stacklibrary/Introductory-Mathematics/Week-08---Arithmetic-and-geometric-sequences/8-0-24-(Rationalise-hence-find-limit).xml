    <questionvariables>
      <text>a:rand([2,3,4,5,6]);
b:rand([-4,-3,-2,-1,1,2,3,4]);
k:rand([0,1,2,3,4]);
numer2:ev(expand((a*n+b)*(1/a)-b/a+k),simp);
ta1: unary_minus_sort(ev((1/a),simp)+ev((-b/a+k),simp)/(a*n+b));
ta2: limit(ta1,n,inf);
polypart: 1/a;
fracpart:(-b/a+k)/(a*n+b);</text>
