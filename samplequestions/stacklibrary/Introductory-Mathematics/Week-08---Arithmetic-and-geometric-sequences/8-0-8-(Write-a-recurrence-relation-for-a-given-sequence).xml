    <questionvariables>
      <text>k: rand([1,2,3,4,5,6,7])
d: rand([2,3,4])
a1: k+d
an: ev(ratsimp(a1+(n-1)*d))
N:5
S1:ev(makelist(k,k,1,N),simp)
S2:maplist(lambda([ex],ev(an,n=ex)),S1)
S3:ev(S2,simp)
an_1:ev(an,n=n+1)
ta1: ev(a1,simp)
ta2: a_n+d</text>
