 d_magnitude:rand(7)+1;
 d_sign:2*rand(2)-1;
 dval:d_sign*d_magnitude;
 mvalminus1:mval - 1;
 nval:mval + rand(4) + 2;
 nvalminus1:nval - 1;
 un:a+(n-1)*d;
 mproduct:mvalminus1*d;
 nproduct:nvalminus1*d;
 mproductval:mvalminus1*dval;
 nproductval:nvalminus1*dval;
 term_mval:aval + mproductval;
 term_nval:aval + nproductval;
 n_minus_m:nval - mval;
 n_minus_md:n_minus_m*d;
 np1:nval + 1;</text>
