    <questionvariables>
      <text><![CDATA[seqs:[ [1, makelist(1/2+(1/2)*(-1)^n,n,1,8),"Neither"], [2, makelist(n^2,n,1,8),"Increasing"], [3, makelist(1/n,n,1,8),"Decreasing"], [4, makelist(ceiling(n/2)*(-1)^(n+1),n,1,8),"Neither"], [5, makelist(3,n,1,8),"Neither"], [6, [1,3,2,4,3,5,4,6],"Neither"], [7, makelist(ceiling((10+n)/2),n,1,8),"Neither"], [8,[0,1,0,2,0,3,0,4],"Neither"], [9, makelist(1-1/2^n,n,1,8),"Increasing"], [10, makelist(-2*n,n,1,8),"Decreasing"] ];
seqs:random_permutation(seqs);
version:maplist(first, seqs);
un:maplist(second, seqs);
corr:maplist(third, seqs);
opts:["Increasing", "Decreasing", "Neither", "Both"];
create_mcq:lambda([corr], maplist(lambda([ex],[ex, is(ex=corr)]), opts))
mcq:maplist(lambda([ex],create_mcq(ex)), corr);]]></text>
