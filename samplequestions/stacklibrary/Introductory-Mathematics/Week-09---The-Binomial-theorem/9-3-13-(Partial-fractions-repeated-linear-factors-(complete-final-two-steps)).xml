    </stackversion>
    <questionvariables>
      <text>ordergreat(A,B);
a:rand_with_prohib(-5,5,[0,1]);
partialFractionsForm:A/(x-a)+B/(x-a)^2;
A:rand_with_prohib(-5,5,[0]);
B:rand_with_prohib(-5,5,[0,A,-A]);
partialFractions:ev(partialFractionsForm);
ratFnNum:num(ratsimp(partialFractions));
ratFnDen:(x-a)^2;
ratFn:ratFnNum/ratFnDen;
ta1:partialFractions;</text>
