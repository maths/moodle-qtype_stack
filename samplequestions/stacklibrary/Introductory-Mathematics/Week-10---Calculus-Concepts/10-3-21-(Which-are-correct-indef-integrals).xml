    </stackversion>
    <questionvariables>
      <text><![CDATA[n:rand([3,4,5,6]);
corbase:['int(cos(x),x)=sin(x)+C, 'int(x^(n-1),x)=(x^n)/n+C, 'int(exp(x),x)=exp(x)+C];
/* note - the first entry in wrongbase is rewritten to have a +C but no dx using string replacement below... */
wrongbase:['int(sin(x),x)=-cos(x), 'int(x^n,x)=n*x^(n-1)+C, 'int(x^ev((n-1),simp),x)=(x^(n-1))/(n-1)];
mcq:multiselqnalpha(corbase, 2, wrongbase, 3);
ta1:maplist(lambda([ex],[ex[1],ex[2],ssubst("\\(\\displaystyle\\int \\sin(x) = -\\cos(x)+C \\)","\\(\\displaystyle \\int {\\sin \\left( x \\right)}{\\;\\mathrm{d}x}=-\\cos \\left( x \\right)\\)",ex[3])]),mcq[1]);
version:mcq[2];
corr1:mcq_correct(ta1);
incorr1:mcq_incorrect(ta1);]]></text>
