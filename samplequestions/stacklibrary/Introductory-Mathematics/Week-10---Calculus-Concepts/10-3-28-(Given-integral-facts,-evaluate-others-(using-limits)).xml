    <questionvariables>
      <text><![CDATA[simp:true;
a:rand_range(-4,-1);
b:rand_with_prohib(a+1,2,[0]);
c:rand_range(b+1,5);
A:rand_range(5,10);
B:rand_with_prohib(5,15,[A]);
C:rand_with_prohib(1,15,[A,B]);
D:rand_with_prohib(1,15,[A,B,D]);
n:rand_range(2,4);
m:rand_range(2,5);

/* this determines how we will shuffle the parts: */
reordering:random_permutation([1,2,3,4,5]);
simp:false;

/* a master list of the expressions and the answers, given as equations */
shorthand: 
[Fab='int(f(x),x,a,b),Fac='int(f(x),x,a,c),Gab='int(g(x),x,a,b),Gbc='int(g(x),x,b,c)];
expr_master:['int(f(x),x,b,c)=Fac-Fab, 'int((n*f(x)+m*g(x)),x,a,b)=n*Fab+m*Gab, 'int(f(x)-g(x),x,a,c)=Fac-(Gab+Gbc), 'int(x*f(x),x,a,b)=none,  'int(x*f(x),x,a,a)=0];

/* perform the reordering */

expr:makelist(lhs(expr_master[i]),i,reordering);
ta:maplist(rhs, makelist(ev(expr_master[i],[Fab=A,Fac=B,Gab=C,Gbc=D]),i,reordering));
taworking:map(lambda([ex],ex=ev(ex,simp)),ta); 

 /* produce lines of working */
ta:map(lambda([ex],ev(ex,simp)),ta);

 /* now simplify the final answers */
working:maplist(rhs, makelist(ev(expr_master[i],shorthand),i,reordering));

/* here we finesse the two unusual cases */

ans_zero:sublist_indices(ta, lambda ([ex], ex=0))[1];
ans_none:sublist_indices(ta, lambda ([ex], ex=none))[1];
taworking[ans_zero]:"";
taworking[ans_none]:"";
continues:["=","=","=","=","="];
continues[ans_zero]:"}\\), since the upper and lower limits of the definite integral are the same\\({";
continues[ans_none]:"}\\), since we don't have enough information about the integrand\\({";

ta1:ta[1];
ta2:ta[2];
ta3:ta[3];
ta4:ta[4];
ta5:ta[5];]]></text>
