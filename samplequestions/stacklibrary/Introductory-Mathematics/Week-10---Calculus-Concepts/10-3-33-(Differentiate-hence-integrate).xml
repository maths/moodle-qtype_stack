    <questionvariables>
      <text>v:t;
a:rand_with_prohib(-3,2,[0]);
b:rand_with_prohib(a+1,4,[0]);
m:rand_range(1,4);
n:rand_with_prohib(1,4,[m,4*(m-1)]); /* 4*(m-1) prevents n=2m when m=2 */
[p1,p2]:rand([[3,2],[4,2],[4,3],[3,1],[4,1]]);
g:m*v^p1+n*v^p2;
c:rand_range(2,3);
fdash:expand(c*g);
f:int(fdash,v);
gint:expand(f/c);
ta1:fdash;
ta2:int(g,v,a,b);</text>
