    </stackversion>
    <questionvariables>
      <text><![CDATA[v: rand([1,2,3]);
k_val: [[1,4], [2,8], [4,9]][v];
con: [[3,2],[3*sqrt(2),4], [5,6]][v];
a: con[1];
b:1;
c: con[2];
k1: k_val[1]
k2: k_val[2];

p1: a/(2*sqrt(x));
p2: b;
f: a/(2*sqrt(x))-b;
int_f: integrate(f,x);
assume(k>0);
f_k: integrate(f,x,0,k);

ta1: set(k1,k2);]]></text>
